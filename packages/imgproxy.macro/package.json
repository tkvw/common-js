{
  "name": "@tkvw/imgproxy.macro",
  "version": "1.0.0",
  "description": "Babel macro to generate signed imgproxy urls.",
  "main": "dist/macro.js",
  "files": [
    "dist",
    "macro.js"
  ],
  "prettier": {
    "singleQuote": false
  },
  "scripts": {
    "describe": "npm-scripts-info",
    "build": "run-s clean && run-p build:*",
    "build:main": "babel -d dist src",
    "test": "run-s build test:*",
    "test:unit": "jest --coverage",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "trash dist",
    "all": "run-s reset test",
    "prepare-release": "run-s all version"
  },
  "keywords": [
    "imgproxy",
    "babel",
    "babel-macros",
    "babel-plugin-macros"
  ],
  "author": "Tkvw/Dennie de Lange",
  "jest": {
    "testRegex": ".*.spec.js$"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-plugin-tester": "^6.1.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "trash-cli": "^2.0.0"
  },
  "dependencies": {
    "babel-plugin-macros": "^2.5.1"
  }
}
